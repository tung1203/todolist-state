odo.js","original":{"line":130,"column":9}},{"generated":{"line":80,"column":20},"source":"src/containers/Modal/EditTodo.js","original":{"line":130,"column":4}},{"generated":{"line":81,"column":0},"source":"src/containers/Modal/EditTodo.js","original":{"line":131,"column":3}}],"sources":{"src/containers/Modal/EditTodo.js":"import task from \"../../store/task\";\r\nimport Button from \"../../components/Button\";\r\nexport default class EditTodo {\r\n  constructor(element) {\r\n    this.element = element;\r\n    this.editTask = this.editTask.bind(this);\r\n  }\r\n  editTask(id) {\r\n    id = parseInt(id);\r\n    let title = document.getElementById(\"title-edit\").value;\r\n    let description = document.getElementById(\"description-edit\").value;\r\n    let from = document.getElementById(\"from-edit\").value;\r\n    let to = document.getElementById(\"to-edit\").value;\r\n    let newTask = {\r\n      id,\r\n      title,\r\n      description,\r\n      from,\r\n      to,\r\n      complete: false,\r\n    };\r\n\r\n    task.editTask(newTask);\r\n\r\n    document.getElementById(\"title-edit\").value = \"\";\r\n    document.getElementById(\"description-edit\").value = \"\";\r\n    document.getElementById(\"from-edit\").value = \"\";\r\n    document.getElementById(\"to-edit\").value = \"\";\r\n  }\r\n  handleDOM() {\r\n    const addTaskEl = document.querySelector(\"#btn-edit-task\");\r\n    const that = this;\r\n    addTaskEl &&\r\n      addTaskEl.addEventListener(\"click\", function (e) {\r\n        that.editTask(e.target.getAttribute(\"data-id\"));\r\n      });\r\n  }\r\n  render() {\r\n    return `\r\n      <div class=\"modal-dialog\">\r\n        <div class=\"modal-content p-3\">\r\n          <header class=\"head-form mb-0 mt-4 text-center\">\r\n            <h2 id=\"header-title\">Edit Task</h2>\r\n          </header>\r\n\r\n          <!-- Modal body -->\r\n          <div class=\"modal-body\">\r\n            <form role=\"form\">\r\n              <div class=\"form-group row\">\r\n                <div class=\"input-group\">\r\n                  <label\r\n                    for=\"example-datetime-local-input\"\r\n                    class=\"col-2 col-form-label\"\r\n                    >Title</label\r\n                  >\r\n                  <input\r\n                    type=\"text\"\r\n                    name=\"title\"\r\n                    id=\"title-edit\"\r\n                    class=\"col-10 form-control input-sm\"\r\n                    placeholder=\"Title\"\r\n                  />\r\n                </div>\r\n\r\n                <span class=\"sp-thongbao\" id=\"tbMaNV\"></span>\r\n              </div>\r\n\r\n              <div class=\"form-group row\">\r\n                <label\r\n                  for=\"example-datetime-local-input\"\r\n                  class=\"col-2 col-form-label\"\r\n                  >Description</label\r\n                >\r\n                <input\r\n                  type=\"text\"\r\n                  name=\"description\"\r\n                  id=\"description-edit\"\r\n                  class=\"col-10 form-control input-sm\"\r\n                  placeholder=\"Description\"\r\n                />\r\n              </div>\r\n              <div class=\"form-group row\">\r\n                <label\r\n                  for=\"example-datetime-local-input\"\r\n                  class=\"col-2 col-form-label\"\r\n                  >From</label\r\n                >\r\n                <input\r\n                  class=\"form-control col-10\"\r\n                  type=\"datetime-local\"\r\n                  id=\"from-edit\"\r\n                />\r\n              </div>\r\n              <div class=\"form-group row\">\r\n                <label\r\n                  for=\"example-datetime-local-input\"\r\n                  class=\"col-2 col-form-label\"\r\n                  >Due date</label\r\n                >\r\n                <input\r\n                  class=\"form-control col-10\"\r\n                  type=\"datetime-local\"\r\n                  id=\"to-edit\"\r\n                />\r\n              </div>\r\n            </form>\r\n          </div>\r\n\r\n          <!-- Modal footer -->\r\n          <div class=\"modal-footer\" id=\"modal-footer\">\r\n          ${Button({\r\n            text: \"Đóng\",\r\n            color: \"btn-secondary\",\r\n            id: \"btnDong\",\r\n            dataDismiss: \"modal\",\r\n          })}\r\n          ${Button({\r\n            text: \"Edit\",\r\n            color: \"btn-success\",\r\n            id: \"btn-edit-task\",\r\n            dataDismiss: \"modal\",\r\n          })}\r\n          </div>\r\n        </div>\r\n      </div>\r\n    `;\r\n  }\r\n  init() {\r\n    this.element.innerHTML = this.render();\r\n    this.handleDOM();\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"c2e5082cc9f09b2a2ba52edcb8f8ec38","cacheData":{"env":{}}}